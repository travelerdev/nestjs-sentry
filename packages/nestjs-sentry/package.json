{
  "contributors": [
    {
      "name": "Dexter Hardy",
      "email": "dexter.hardy@ntegral.com",
      "url": "http://www.ntegral.com"
    },
    {
      "name": "Zack Sheppard",
      "email": "zack@traveler.dev",
      "url": "https://www.traveler.dev"
    }
  ],
  "name": "@travelerdev/nestjs-sentry",
  "version": "4.3.0",
  "description": "Provides an injectable sentry.io client to provide enterprise logging of nestjs modules",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "directories": {
    "dist": "dist",
    "lib": "lib"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/travelerdev/nestjs-sentry"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "clean": "rm -rf dist",
    "format": "prettier --write \"lib/**/*.ts\"",
    "publish:npm": "npm publish --access public",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
  },
  "keywords": [
    "nestjs",
    "sentry.io"
  ],
  "author": "Zack Sheppard and Dexter Hardy",
  "license": "ISC",
  "peerDependencies": {
    "@nestjs/common": "^9.0.0 || ^10.0.0",
    "@sentry/hub": "^7.12.0",
    "@sentry/node": "^7.12.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0"
  },
  "devDependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@sentry/hub": "^7.56.0",
    "@sentry/node": "^7.56.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "lib",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
